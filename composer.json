{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/Yismen/cors.git"
        },
        { 
            "type": "git", 
            "url": "https://github.com/Yismen/clear-log-files.git" 
        } 
    ],

    "require": {
        "php": "^7.1.3",
        "arcanedev/log-viewer": "7.*",
        "cviebrock/eloquent-sluggable": "7.*",
        "dainsys/clear-laravel-logs": "^2.1",
        "doctrine/dbal": "^2.8",
        "facade/ignition": "^2.0",
        "fideloper/proxy": "^4.0",
        "fruitcake/laravel-cors": "^1.0",
        "graham-campbell/flysystem": "^7.0",
        "guzzlehttp/guzzle": "~5.3|~6.0",
        "intervention/image": "*",
        "laravel/framework": "^7.0",
        "laravel/passport": "^8.0",
        "laravel/telescope": "~3.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.0",
        "laravelcollective/html": "^6.0",
        "league/flysystem-sftp": "^1.0",
        "maatwebsite/excel": "^3.0",
        "pusher/pusher-php-server": "^4.1",
        "spatie/flysystem-dropbox": "^1.0",
        "spatie/laravel-backup": "^6.9",
        "spatie/laravel-permission": "^3.0",
        "tucker-eric/eloquentfilter": "^2.3",
        "yajra/laravel-datatables-oracle": "^9.0"
    },
    "require-dev": {
        "filp/whoops": "^2.1",
        "fzaninotto/faker": "^1.4",
        "laravel/dusk": "^5.5",
        "laravel/homestead": "^10",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Http/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": ["tests/utilities/functions.php"],        
        "classmap": [
            "tests/Feature",
            "tests/Unit"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "php artisan clear-compiled"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled"
        ],
        "production": [
            "composer dev",
            "php artisan route:cache",
            "php artisan view:cache"
        ],
        "dev": [
            "php artisan clear-compiled",
            "php artisan route:clear",
            "php artisan config:clear",
            "php artisan view:clear"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
